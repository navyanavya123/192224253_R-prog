table1 <- data.frame(a = c(1, 2, 3), b = c(4, 5, 6), c = c(7, 8, 9), d = c(10, 11, 12))
table2 <- data.frame(a = c(13, 14, 15), b = c(16, 17, 18), c = c(19, 20, 21), d = c(22, 23, 24))
mat <- cbind(table1, table2)
array1 <- array(mat, dim = c(3, 4, 2))
print(array1)
arr1 <- seq(52, 100, 2)
array2 <- array(arr1, dim = c(5, 3))
print(array2)



m=matrix(11:20,nrow=5,ncol=4,byrow=TRUE)
rn=c("r1","r2","r3","r4","r5")
cn=c("c1","c2","c3","c4")
rownames(m)=rn
colnames(m)=cn
print(m)

mm=matrix(25:33,nrow=3,ncol=3,byrow=TRUE)
rnn=c("r1","r2","r3")
cnn=c("c1","c2","c3")
rownames(mm)=rnn
colnames(mm)=cnn
print(mm)

mr=matrix(1:4,nrow=2,ncol=2,byrow=TRUE)
rnk=c("r1","r2")
cnk=c("c1","c2")
rownames(mr)=rnk
colnames(mr)=cnk
print(mr)

